{
    "name": "ewinslow/elgg-evan",
    "description": "Provides some experimental development conveniences for Elgg",
    "type": "elgg-plugin",
    "extra": {
        "installer-name": "evan"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Winslow",
            "email": "evan.b.winslow@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Evan\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~5.4",
        "composer/installers": "~1.0.8",
        "elgg/elgg": "~1.9",
        "mnapoli/php-di": "~4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    }
}
